<% include header %>
            <div class="posts">
                <section class="post">
                    <header class="post-header">
                        <h2 class="post-title"><%= title %></h2>

                        <p class="post-meta">
                            On <a href="#" class="post-author"><%= createdAt %></a> <%= (tags ? 'Under' : '') %> 
								<% tags && _.each(tags, function(tag){%>
									<a class="post-category post-category-design" href="#"><%= tag %></a> 
								<%})%>
                        </p>
                    </header>
                    <div class="post-description">
                       <%= content %>
                    </div>
                </section>
				
				<p>All Posts</p>
				<ul>
				<% posts.each(function(post){%>
					<h2 class="post_title"><a href="<%= encodeURI(post.get('title')) %>.html"><%= post.get('title') %></a></h2>
				<%})%>
				</ul>
            </div>
 <% include footer %>
